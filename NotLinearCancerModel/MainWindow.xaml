<Window x:Class="NotLinearCancerModel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NotLinearCancerModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1300">
    <Grid Margin="0,0,0,0" Width="1300" >
        <Grid.RowDefinitions>
            <RowDefinition Height="0*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="Calculate"
                Click="Calculate_Click"
                Content="Calculate" 
                HorizontalAlignment="Left" 
                Margin="20,645,0,0" 
                VerticalAlignment="Top" RenderTransformOrigin="0.768,1.543" Grid.RowSpan="2" Width="61"/>
        <TextBox 
            x:Name="TextBoxLength" 
            HorizontalAlignment="Left" 
            Margin="109,647,0,0" 
            Text="250" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            Width="120" Grid.RowSpan="2"/>
        <TextBox 
            x:Name="TextBoxH" 
            HorizontalAlignment="Left" 
            Margin="255,647,0,0" 
            Text="10" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            Width="120"
            Grid.RowSpan="2"/>
        <TextBox 
            x:Name="TextBoxD" 
            HorizontalAlignment="Left" 
            Margin="399,647,0,0" 
            Text="10e-12" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            Width="120" Grid.RowSpan="2"/>
        <TextBox x:Name="TextBoxSpeed" 
            HorizontalAlignment="Left" 
            Margin="109,603,0,0" 
            Text="1.3" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            Width="120" Grid.RowSpan="2"/>
        <TextBox x:Name="TextBoxAngleXY" 
            HorizontalAlignment="Left" 
            Margin="255,603,0,0" 
            Text="30" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            Width="120" Grid.RowSpan="2"/>
        <TextBox x:Name="TextBoxAngleZ" 
            HorizontalAlignment="Left" 
            Margin="399,603,0,0" 
            Text="30" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            Width="120" Grid.RowSpan="2"/>
        <TextBox 
            x:Name="TextBoxK" 
            HorizontalAlignment="Left" 
            Margin="545,603,0,0" 
            Text="0.4" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            Width="120" Grid.RowSpan="2"/>
        <TextBox 
            x:Name="TextBoxAccuracy" 
            HorizontalAlignment="Left" 
            Margin="689,603,0,0" 
            Text="1" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            Width="120"
            Grid.RowSpan="2"/>
        <TextBox 
            x:Name="TextBoxStepAccuracy" 
            HorizontalAlignment="Left" 
            Margin="689,647,0,0" 
            Text="0.1" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            Width="120" Grid.RowSpan="2"/>
        <Label 
            x:Name="LabelLength" 
            Content="Length" 
            HorizontalAlignment="Left" 
            Margin="109,668,0,0" 
            VerticalAlignment="Top" Grid.RowSpan="2"/>
        <Label 
            x:Name="LabelH" 
            Content="H" 
            HorizontalAlignment="Left" 
            Margin="255,668,0,0" 
            VerticalAlignment="Top" Grid.RowSpan="2"/>
        <Label 
            x:Name="LabelD" 
            Content="D" 
            HorizontalAlignment="Left" 
            Margin="399,669,0,0" 
            VerticalAlignment="Top" Grid.RowSpan="2"/>
        <Label x:Name="LabelK" Content="K" HorizontalAlignment="Left" Margin="545,621,0,0" Grid.RowSpan="2" VerticalAlignment="Top" RenderTransformOrigin="0.839,-0.32"/>
        <Label x:Name="LabelSpeed" Content="Speed" HorizontalAlignment="Left" Margin="109,621,0,0" Grid.RowSpan="2" VerticalAlignment="Top"/>
        <Label x:Name="LabelAngleXY" Content="AngleXY" HorizontalAlignment="Left" Margin="255,621,0,0" Grid.RowSpan="2" VerticalAlignment="Top"/>
        <Label x:Name="LabelAngleZ" Content="AngleZ" HorizontalAlignment="Left" Margin="399,621,0,0" Grid.RowSpan="2" VerticalAlignment="Top"/>
        <Label x:Name="LabelAccuracy" Content="Accuracy" HorizontalAlignment="Left" Margin="689,621,0,0" Grid.RowSpan="2" VerticalAlignment="Top" RenderTransformOrigin="0.491,0.013"/>
        <Label x:Name="LabelStepAccuracy" Content="StepAccuracy" HorizontalAlignment="Left" Margin="689,669,0,0" Grid.RowSpan="2" VerticalAlignment="Top"/>
        <Image x:Name="Image1" HorizontalAlignment="Left" Height="511" Margin="40,50,0,0" Grid.RowSpan="2" VerticalAlignment="Top" Width="600"/>
        <Image x:Name="Image2" HorizontalAlignment="Left" Height="511" Margin="660,50,0,0" Grid.RowSpan="2" VerticalAlignment="Top" Width="600"/>
        <!--<WindowsFormsHost x:Name="WFHost1" HorizontalAlignment="Left" Height="502" Margin="20,40,0,0" Grid.RowSpan="2" VerticalAlignment="Top" Width="500" Loaded="WFHost1_Loaded"/>-->
        <Button x:Name="ShowPlots" Content="Show Plots" HorizontalAlignment="Left" Margin="20,603,0,0" Grid.RowSpan="2" VerticalAlignment="Top" Width="61" Height="18" Click="ShowPlots_Click"/>

    </Grid>
</Window>
